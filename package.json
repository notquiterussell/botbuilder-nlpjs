{
  "name": "middleware-test",
  "version": "1.0.0",
  "description": "Middleware to delegate to nlp.js from Microsoft botbuilder",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha 'test/**/*.js' --recursive --timeout 60000 --exit",
    "train": "node src/train",
    "smalltalk": "node src/train/smalltalk"
  },
  "author": "Russell Geraghty <russell@rosesareblue.co.uk>",
  "license": "MIT",
  "dependencies": {
    "@botbuildercommunity/middleware-engine-core": "^1.1.0",
    "botbuilder": "^4.11.0",
    "node-nlp": "^4.17.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "csv-parse": "^4.14.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  }
}
